{
  "name": "@coldiron/netherite",
  "version": "0.0.0",
  "tasks": {
    "dev": "deno run --allow-read --allow-write --allow-run --allow-env src/cli/cli.ts",
    "init": "netherite init --name 'Test' --author 'Caleb Coldiron' --namespace 'cld_test' --formatVersion 'latest' --type 'world' --script 'deno'",
    "install": "deno install -f -g --name netherite --allow-read --allow-write --allow-run --allow-env src/cli/cli.ts",
    "test": "rm -rf Test && deno task install && deno task init"
  },
  "exports": {
    "./cli": "./src/cli.ts",
    "./api": "./src/api.ts"
  },
  "imports": {
    "@luca/esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@^0.11.1",
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.11",
    "@std/uuid": "jsr:@std/uuid@^1.0.4",
    "esbuild": "npm:esbuild@^0.24.2"
  }
}
